          "_customHTML": "<section class=\"footer4\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked>\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" name=\"iconColor\" value=\"#5b6c8f\" condition=\"showSocial\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background8.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#f0f2f5\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row mbr-white\">\n            <div class=\"col-12 col-md-6 col-lg-2\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Information</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\">About us\n</li><li class=\"mbr-text item-wrap\">Delivery&nbsp;</li><li class=\"mbr-text item-wrap\">Privacy Policy\n</li><li class=\"mbr-text item-wrap\">Sales\n</li><li class=\"mbr-text item-wrap\">Conditions</li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-2\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Account</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n\n                    <li class=\"mbr-text item-wrap\">My account\n</li><li class=\"mbr-text item-wrap\">My orders\n</li><li class=\"mbr-text item-wrap\">Returns\n</li><li class=\"mbr-text item-wrap\">Shipping\n</li><li class=\"mbr-text item-wrap\">Wishlist</li>\n                </ul>\n            </div>\n              <div class=\"col-12 col-md-6 col-lg-2\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Store</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n\n                    <li class=\"mbr-text item-wrap\">Affiliate\n</li><li class=\"mbr-text item-wrap\">Bestsellers\n</li><li class=\"mbr-text item-wrap\">Discount\n</li><li class=\"mbr-text item-wrap\">Latest products\n</li><li class=\"mbr-text item-wrap\">Sale</li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-4\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\"><b>Need help</b></h5>\n                <h6 mbr-text class=\"phone mbr-fonts-style mb-2\" data-app-selector=\".phone\" mbr-theme-style=\"display-5\"><b>989-879-0847</b></h6>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .mbr-section-btn\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\">Monday â Friday: 9:00-20:00\n</li><li class=\"mbr-text item-wrap\">Saturday: 11:00 â 15:00</li>\n                </ul>\n                 <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn mt-2 mb-4\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-md btn-white\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">contact@yoursite.com</a></div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-2\">\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\"><b>Our store</b></h5>\n                <p class=\"mbr-text mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Orlando 1307  Terry Lane 45</p>\n                <p class=\"mbr-text map mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Show on map</p>\n                <div class=\"social-row display-7\" mbr-if=\"showSocial\">\n                    <div class=\"soc-item\">\n      